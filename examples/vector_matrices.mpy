# Vectors
    v1 = [1, 2, 3]
    v2 = [4, 5, 6]

    # Element-wise addition
    v_sum = v1 .+ v2

    # Matrices
    A = [
        [1, 2],
        [3, 4]
    ]

    B = [
        [5, 6],
        [7, 8]
    ]

    # Matrix multiplication
    C = A * B

    print("Vector Sum:", v_sum)
    print("Matrix Product:")
    print(C)